{% raw %}
class {{ class_name }}:
    def __init__(self, driver):
        self.driver = driver

    def open_page(self):
        self.driver.get("{{ url }}")

    def click_element(self, element):
        self.driver.find_element_by_id(element).click()

    def enter_text(self, element, text):
        self.driver.find_element_by_id(element).send_keys(text)
